<h1>Contacts</h1>
<div class="table-responsive">
  <table class="table">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Company</th>
        <th>Title </th>
        <th>Email </th>
        <th>Phone </th>
        <th>URL </th>
        <th>About </th>
      </tr>
    </thead>
    <tbody>
      <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.id %>
        <td><%= contact.fname %> <%= contact.lname %></td>
        <td><%= contact.company.name %></td>
        <td><%= contact.title %></td>
        <td><%= contact.email %></td>
        <td><%= contact.phone %></td>
        <td><%= contact.url %></td>
        <td><%= contact.about %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>